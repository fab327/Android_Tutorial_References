<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.justfabcodes.rocketlauncher">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.justfabcodes.rocketlauncher.MainListActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.NoAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.LaunchRocketValueAnimatorAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.RotateRocketAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.AccelerateRocketAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.LaunchRocketObjectAnimatorAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.ColorAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.LaunchAndSpinAnimatorSetAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.LaunchAndSpinViewPropertyAnimatorAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.FlyWithDogeAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.WithListenerAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.FlyThereAndBackAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.XmlAnimationActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.justfabcodes.rocketlauncher.animationactivities.PhysicsAnimationActivity"
            android:label="@string/app_name" />

    </application>

</manifest>
